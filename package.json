{
  "name": "@tomyitav/graphql-server-typed",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build-schema:dev": "rimraf ./dist/graphql/schema/ && dir /s /b \"src/graphql/types\" | findstr /e .ts > ts-files.txt && tsc ./src/graphql/schema/genereator-schema.ts --outDir ./dist/graphql/schema/ --lib esnext && tsc @ts-files.txt --outDir ./dist/graphql/types --lib esnext && del ts-files.txt",
    "generate-ts": "gql-gen --template ts --out ./src/interfaces/types.d.ts --schema ./dist/graphql/schema/genereator-schema.js",
    "pregenerate:dev": "npm run build-schema:dev",
    "generate:dev": "npm run generate-ts",
    "clean": "rimraf dist",
    "prebuild:dev": "npm run clean && npm run generate:dev",
    "build:dev": "npm run tsc:dev",
    "start": "node dist/main.js",
    "start:dev": "ts-node src/main.ts",
    "dev": "nodemon",
    "test": "tsc && mocha dist/**/*.spec.js",
    "lint": "eslint src --ext ts",
    "tsc:dev": "tsc",
    "build-schema": "rimraf ./dist/graphql/schema/ && find src/graphql/types -iname \"*.ts\" > ts-files.txt && tsc ./src/graphql/schema/genereator-schema.ts --outDir ./dist/graphql/schema/ --lib esnext && tsc @ts-files.txt --outDir ./dist/graphql/types --lib esnext && rimraf ts-files.txt",
    "pregenerate": "npm run build-schema",
    "generate": "npm run generate-ts",
    "prebuild": "npm run clean && npm run generate",
    "build": "npm run tsc",
    "tsc": "tsc",
    "tslint": "tslint --fix -c tslint.json 'src/**/*.ts'",
    "prettier": "prettier --write \"src/**/*.ts\"",
    "precommit-tslint": "tslint --fix -c tslint.json",
    "precommit-prettier": "prettier --write",
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "deploy": "now"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "*.ts": ["npm run precommit-tslint", "npm run precommit-prettier", "git add"]
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "apollo-server-express": "^2.1.0",
    "body-parser": "^1.18.3",
    "core-js": "^2.5.7",
    "cors": "^2.8.0",
    "express": "^4.16.4",
    "graphql": "^14.0.2",
    "graphql-subscriptions": "^0.5.6",
    "graphql-tools": "^4.0.2",
    "injection-js": "^2.2.1",
    "lodash": "^4.17.11",
    "merge-graphql-schemas": "^1.5.7",
    "reflect-metadata": "^0.1.10",
    "subscriptions-transport-ws": "^0.9.15",
    "winston": "^2.4.4",
    "winston-daily-rotate-file": "^1.4.6",
    "zone.js": "^0.8.18"
  },
  "devDependencies": {
    "@types/express": "^4.16.0",
    "@types/graphql": "^14.0.3",
    "@types/lodash": "^4.14.117",
    "@types/mocha": "2.2.41",
    "@types/node": "7.0.22",
    "graphql-code-generator": "^0.12.5",
    "graphql-codegen-typescript-template": "^0.12.5",
    "eslint": "4.0.0",
    "eslint-config-airbnb-base": "11.2.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-promise": "3.5.0",
    "express-serve-static-core": "^0.1.1",
    "husky": "^1.1.2",
    "lint-staged": "^7.3.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.4",
    "react-dom": "^15.6.0",
    "rimraf": "^2.6.2",
    "supertest": "3.3.0",
    "ts-node": "^7.0.1",
    "tsc": "1.20150623.0",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-react": "^3.6.0",
    "typescript": "^2.9.2",
    "typescript-eslint-parser": "3.0.0"
  }
}
